    "last 2 versions",
    "not ie < 11",
    "not ie_mob < 11",
    "not dead"
  ],
  "scripts": {
    "test:endtoend": "jest -c ./endtoend/jest.conf.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "format": "prettier --write '**/*.{js,jsx,json,md}'",
    "format:changed": "pretty-quick",
    "format:staged": "pretty-quick --staged",
    "lint": "echo kek",
    "lint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:js": "eslint src/**/*.js -c .eslintrc.json",
    "lint:css": "stylelint 'src/**/*.{scss,css}' && stylelint --rd 'src/**/*.{scss,css}'",
    "lint:dup": "jscpd -g --threshold=1 tests docker",
    "clean": "rm -rf build",
    "test": "react-scripts test & npm run test:endtoend",
    "test:engine": "check-engine",
    "test:sec": "npm audit || true",
    "test:unit": "node scripts/test.js",
    "ci": "npm run test && npm run build && npm run test:sec",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format:staged"
    }
  },
  "engines": {
    "npm": ">=3",
    "node": ">=8"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "lodash",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "spec": true
        }
      ],
      "babel-plugin-styled-components",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
